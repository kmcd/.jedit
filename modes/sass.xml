    HIGHLIGHT_DIGITS="TRUE"
    NO_WORD_SEP="-_%">
		<MARK_FOLLOWING TYPE="KEYWORD1">.</MARK_FOLLOWING>
    <SEQ AT_WORD_START="TRUE" TYPE="LITERAL2">!important</SEQ>
    <SEQ_REGEXP TYPE="COMMENT4">#([0-9a-fA-F]{3}){1,2}</SEQ_REGEXP>
    <SEQ_REGEXP TYPE="KEYWORD4">#[A-Za-z][A-Za-z0-9-_]*</SEQ_REGEXP>
    <SEQ_REGEXP TYPE="OPERATOR">-?[\p{Digit}]*\.?[\p{Digit}]+(pt|pc|in|mm|cm|em|ex|px|ms|s|%)</SEQ_REGEXP>
    
    <SEQ_REGEXP TYPE="KEYWORD2" AT_WHITESPACE_END="TRUE">abbr|acronym|address|applet|area|a|base|basefont|bdo|big|blockquote|body|br|button|b|caption|center|cite|code|col|colgroup|dd|del|dfn|dir|div|dl|dt|em|fieldset|font|form|frame|frameset|h[0-6]|head|hr|html|iframe|img|input|ins|isindex|i|kbd|label|legend|link|li|map|menu|meta|noframes|noscript|object|ol|optgroup|option|param|pre|p|q|samp|script|select|small|span|strike|strong|style|sub|sup|s|table|tbody|td|textarea|tfoot|thead|th|title|tr|tt|ul|u|var|xmp</SEQ_REGEXP>
    
		<MARK_FOLLOWING TYPE="KEYWORD3">!</MARK_FOLLOWING>
